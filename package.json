{
  "name": "vnp-bt-auto",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "ui": "node runner.js",
    "electron": "electron .",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "puppeteer": "^22.15.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "electron": "^31.2.1",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.vnp.btauto",
    "productName": "Braintree Auto",
    "files": [
      "**/*",
      "!dist/**",
      "!**/*.map",
      "!.git/**",
      "!**/.DS_Store",
      "!node_modules/.cache/**"
    ],
    "asarUnpack": [
      "node_modules/puppeteer{,/**}",
      "node_modules/puppeteer-core{,/**}",
      "**/*.node"
    ],
    "extraResources": [
      {
        "from": "uploads",
        "to": "uploads",
        "filter": [
          "**/*"
        ]
      }
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}
